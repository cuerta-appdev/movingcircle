<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Furniture Pictures

    <a data-toggle="collapse" href="#furtniture_object_furniture_pictures_new_form">
      Add furniture object picture
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @furtniture_object.furniture_pictures.each do |furniture_object_picture| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <a href="/furniture_object_pictures/<%= furniture_object_picture.id %>">
          <%= furniture_object_picture.created_at %>
        </a>

        <a href="/delete_furniture_object_picture_from_furniture_object/<%= furniture_object_picture.id %>">
          Delete
        </a>

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="furtniture_object_furniture_pictures_new_form">
        <%= render partial: "furtniture_object_templates/furtniture_object_furniture_pictures_new_form" %>
      </div>
    </div>

  </ul>
</div>
