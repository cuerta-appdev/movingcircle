<h1>
  Add a new furniture object picture
</h1>


<hr>

<!-- Form furniture_object_picture start -->
<form action="/create_furniture_object_picture" method="post">
<!-- Form furniture_object_picture end -->

  <!-- Label and input for picture start -->
  <div class="form-group">
    <label for="picture">
      Picture
    </label>

    <!-- Input for picture start -->
    <input type="text" id="picture" name="picture" class="form-control">
    <!-- Input for picture end -->
  </div>
  <!-- Label and input for picture end -->

  <!-- Label and input for furniture_object_id start -->
  <div class="form-group">
    <label for="furniture_object_id">
      Furniture object
    </label>

    <!-- Input for furniture_object_id start -->
    <input type="text" id="furniture_object_id" name="furniture_object_id" class="form-control">
    <!-- Input for furniture_object_id end -->
  </div>
  <!-- Label and input for furniture_object_id end -->

  <button class="btn btn-block btn-outline-secondary">
    Create furniture object picture
  </button>
</form>

<hr>

<a href="/furniture_object_pictures">
  Go back
</a>
