<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Furniture object picture #<%= @furniture_object_picture.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/furniture_object_pictures" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link furniture_object_picture start -->
      <div class="col">
        <a href="/furniture_object_pictures/<%= @furniture_object_picture.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit furniture object picture
        </a>
      </div>
      <!-- Edit link furniture_object_picture end -->

      <!-- Delete link furniture_object_picture start -->
      <div class="col">
        <a href="/delete_furniture_object_picture/<%= @furniture_object_picture.id %>" class="btn btn-block btn-outline-secondary">
          Delete furniture object picture
        </a>
      </div>
      <!-- Delete link furniture_object_picture end -->
    </div>

    <dl>
      <dt>
        Picture
      </dt>
      <!-- Display picture start -->
      <dd>
        <%= @furniture_object_picture.picture %>
      </dd>
      <!-- Display picture end -->

      <dt>
        Furniture object
      </dt>
      <!-- Display furniture_object_id start -->
      <dd>
        <%= @furniture_object_picture.furniture_object_id %>
      </dd>
      <!-- Display furniture_object_id end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@furniture_object_picture.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@furniture_object_picture.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->

<!-- Show Page Customization end -->
