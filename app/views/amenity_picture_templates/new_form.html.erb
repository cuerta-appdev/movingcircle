<h1>
  Add a new amenity picture
</h1>


<hr>

<!-- Form amenity_picture start -->
<form action="/create_amenity_picture" method="post">
<!-- Form amenity_picture end -->

  <!-- Label and input for picture start -->
  <div class="form-group">
    <label for="picture">
      Picture
    </label>

    <!-- Input for picture start -->
    <input type="text" id="picture" name="picture" class="form-control">
    <!-- Input for picture end -->
  </div>
  <!-- Label and input for picture end -->

  <!-- Label and input for description start -->
  <div class="form-group">
    <label for="description">
      Description
    </label>

    <!-- Input for description start -->
    <input type="text" id="description" name="description" class="form-control">
    <!-- Input for description end -->
  </div>
  <!-- Label and input for description end -->

  <!-- Label and input for building_amenity_id start -->
  <div class="form-group">
    <label for="building_amenity_id">
      Building amenity
    </label>

    <!-- Input for building_amenity_id start -->
    <input type="text" id="building_amenity_id" name="building_amenity_id" class="form-control">
    <!-- Input for building_amenity_id end -->
  </div>
  <!-- Label and input for building_amenity_id end -->

  <button class="btn btn-block btn-outline-secondary">
    Create amenity picture
  </button>
</form>

<hr>

<a href="/amenity_pictures">
  Go back
</a>
