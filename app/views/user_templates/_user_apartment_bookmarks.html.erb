<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Apartment Bookmarks

    <a data-toggle="collapse" href="#user_apartment_bookmarks_new_form">
      Add apartment bookmark
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.apartment_bookmarks.each do |apartment_bookmark| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/apartment_bookmarks/<%= apartment_bookmark.id %>">
          <%= apartment_bookmark.user_id %>
        </a>
        <!-- Show link end -->

      </li>
    <% end %>

  </ul>
</div>
